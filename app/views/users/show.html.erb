<%= render partial: 'partials/user', locals: { user: @user } %>
<div class="container">
<%= @user.bio %>
</div>

<% @posts.each do |post| %>
  <%= render partial: 'partials/post', locals: { user: @user, post: post } %>
<% end %>
<div class="btn">
  <%= link_to "See all posts", user_posts_path(@user) %>
</div>

